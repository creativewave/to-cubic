{
  "name": "@cdoublev/to-cubic",
  "description": "A function to transform SVG <path> using cubic commands.",
  "author": "Guillaume Lebas",
  "license": "MIT",
  "keywords": [
    "svg"
  ],
  "version": "0.1.1",
  "homepage": "https://github.com/creativewave/to-cubic#readme",
  "bugs": {
    "url": "https://github.com/creativewave/to-cubic/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativewave/to-cubic.git"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/umd/to-cubic.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:cjs": "BABEL_ENV=cjs babel src -d dist/cjs",
    "build:es": "BABEL_ENV=es babel src -d dist/es",
    "build:umd": "webpack -d src/index.js -o dist/umd/to-cubic.js",
    "build:umd:min": "webpack -p src/index.js -o dist/umd/to-cubic.min.js",
    "lint": "eslint src __tests__",
    "safe-publish": "npm run lint && npm run test && npm run build && npm publish",
    "test": "jest",
    "test:report": "jest -i --reporters='jest-junit'",
    "test:watch": "jest --watchAll -b"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@cdoublev/eslint-config": "^0.2.0",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.8.2",
    "core-js": "^3.4.7",
    "eslint": "^6.7.2",
    "eslint-plugin-compat": "^3.3.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": {
    "production": [
      "last 2 versions",
      "not ie <= 11"
    ],
    "development": [
      "last 1 Chrome version"
    ]
  }
}
